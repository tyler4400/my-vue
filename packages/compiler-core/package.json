{
  "name": "@vue/compiler-core",
  "version": "1.0.0",
  "description": "仿写vue3.4的compiler-core",
  "main": "dist/compiler-core.cjs.js",
  "module": "dist/compiler-core.esm.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/compiler-core.esm.js",
      "require": "./dist/compiler-core.cjs.js"
    }
  },
  "files": [
    "index.js",
    "dist"
  ],
  "sideEffects": false,
  "buildOptions": {
    "name": "VueCompilerCore",
    "formats": [
      "esm-bundler",
      "esm-browser",
      "cjs",
      "global"
    ]
  },
  "dependencies": {
    "@vue/shared": "workspace:^"
  }
}
